{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "starterkit-web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./assets/plugins/jquery-ui/jquery-ui.min.css",
        "./assets/plugins/tether/css/tether.min.css",
        "./assets/css/bootstrap.min.css",
        "./assets/css/font-awesome.min.css",
        "./assets/css/animate.min.css",
        "./assets/css/style-bs4.min.css",
        "./assets/css/style.min.css",
        "./assets/css/style-responsive.min.css",
        "./assets/css/default.css",
        "styles.css"
      ],
      "scripts": [
        "./assets/plugins/pace/pace.min.js",
        "./assets/js/popper.min.js",
        "./assets/plugins/tether/js/tether.min.js",
        "./assets/js/jquery-3.2.1.min.js",
        "./assets/js/jquery-migrate-3.0.0.min.js",
        "./assets/plugins/jquery-ui/jquery-ui.min.js",
        "./assets/js/bootstrap.min.js",
        "./assets/js/jquery.slimscroll.min.js",
        "./assets/js/apps.min.js",
        "scripts.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
